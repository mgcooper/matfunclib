<?xml version="1.0" encoding="utf-8"?>
<CustomKeySet derivedfrom="Mac" modifieddefault="false">
   <Context id="Global">
      <Action id="evaluate-selection">
         <Stroke code="VK_ENTER"/>
      </Action>
      <Action id="eval-cell">
         <Stroke code="VK_F7" shift="on"/>
      </Action>
      <Action id="shift-insert-break"/>
      <Action id="exit-matlab">
         <Stroke alt="on" code="VK_Q" meta="on" sysctrl="on"/>
      </Action>
      <Action id="uncomment">
         <Stroke code="VK_T" meta="on" sysctrl="on"/>
      </Action>
      <Action id="drop-line"/>
      <Action id="new-tab">
         <Stroke alt="on" code="VK_T"/>
      </Action>
      <Action id="go-until-cursor">
         <Stroke code="VK_R" meta="on" shift="on" sysctrl="on"/>
      </Action>
   </Context>
   <Context id="CommandWindow">
      <Action follows="None" id="evaluate-selection">
         <Stroke code="VK_ENTER" meta="on" sysctrl="on"/>
      </Action>
   </Context>
   <Context id="Explorer">
      <Action follows="None" id="evaluate-selection"/>
   </Context>
   <Context id="CommandHistory">
      <Action id="evaluate-selection"/>
   </Context>
   <Context id="HTMLRenderer">
      <Action follows="None" id="evaluate-selection"/>
   </Context>
   <Context id="MATLABEditor">
      <Action follows="None" id="evaluate-selection">
         <Stroke code="VK_ENTER" meta="on" sysctrl="on"/>
      </Action>
      <Action follows="None" id="find-and-replace">
         <Stroke alt="on" code="VK_F" meta="on" sysctrl="on"/>
      </Action>
      <Action follows="None" id="find-with-replace">
         <Stroke code="VK_F" meta="on" sysctrl="on"/>
      </Action>
   </Context>
</CustomKeySet>